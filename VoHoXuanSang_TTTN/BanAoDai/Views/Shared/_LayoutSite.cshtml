        <script src="js/custom.js"></script>*@
    <script src="~/Public/tem/js/jquery-3.2.1.min.js"></script>
    <script src="~/Public/tem/styles/bootstrap4/popper.js"></script>
    <script src="~/Public/tem/styles/bootstrap4/bootstrap.min.js"></script>
    <script src="~/Public/tem/plugins/OwlCarousel2-2.2.1/owl.carousel.js"></script>
    <script src="~/Public/tem/js/custom.js"></script>
    <script src="~/Public/tem/plugins/Isotope/isotope.pkgd.min.js"></script>
    <script src="~/Public/tem/plugins/easing/easing.js"></script>
    @RenderSection("footer", required: false)
</body>

</html>

















